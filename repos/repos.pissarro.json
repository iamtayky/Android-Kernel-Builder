[
  {
    "kernelSource": {
      "name": "Pissarro-ksu",
      "repo": "https://github.com/sonrixmomo/kernel_xiaomi_pissarro",
      "branch": "dragon",
      "device": "pissarro",
      "defconfig": "pissarro_user_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://gitlab.com/LeCmnGend/proton-clang",
        "branch": "clang-13",
        "name": "clang",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "/clang/bin/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "/clang/bin/arm-linux-androideabi-",
        "LD": "clang/bin/ld.lld",
        "AR": "clang/bin/llvm-ar",
        "NM": "clang/bin/llvm-nm",
        "OBJCOPY": "clang/bin/llvm-objcopy",
        "OBJDUMP": "clang/bin/llvm-objdump",
        "READELF": "clang/bin/llvm-readelf",
        "OBJSIZE": "clang/bin/llvm-size",
        "STRIP": "clang/bin/llvm-strip",
        "LDGOLD": "clang/bin/aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "clang/bin/llvm-ar",
        "LLVM_DIS": "clang/bin/llvm-dis",
        "CONFIG_SECTION_MISMATCH_WARN_ONLY": "y"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false,
      "repo": "https://github.com/AbzRaider/AnyKernel33",
      "branch": "pissarro"
    }
  }
]